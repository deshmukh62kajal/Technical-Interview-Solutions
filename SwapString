public class Swap 
{
    public static void main(String args[]) 
    {
        String a = "HELLO";
        String b = "WORLD";

        System.out.println("String Before Swap: a = " + a + " and b = " + b);

        a = a + b;
        b = a.substring(0, a.length() - b.length());
        a = a.substring(b.length());

        System.out.println("\nString After Swap: a = " + a + " and b = " + b);
    }
}
